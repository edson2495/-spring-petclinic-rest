pipeline {

    agent {
        docker {
            image 'maven:3.9.12-eclipse-temurin-17'
        }
    }
    
    
    stages {
        
        stage('Package') {
            steps {
                sh 'mvn clean package -B -ntp -DskipTests'
            }
        }
        
        
    }
    post {
        success {
            archiveArtifacts artifacts: 'target/*.jar', fingerprint: true
        }
        cleanup{
            cleanWs()
        }
    }
}